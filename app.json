{
	"expo": {
		"name": "alarmia",
		"slug": "alarmia",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "alarmia",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			[
				"@react-native-community/datetimepicker",
				{
					"android": {
						"datePicker": {
							"colorAccent": {
								"light": "#FF5722"
							},
							"textColorPrimary": {
								"light": "#FF5722"
							}
						},
						"timePicker": {
							"background": {
								"light": "#FF5722",
								"dark": "#383838"
							},
							"numbersBackgroundColor": {
								"light": "#FF5722",
								"dark": "#383838"
							}
						}
					}
				}
			],
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}